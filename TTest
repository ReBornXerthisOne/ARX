                PaddingLabel1.Parent = Label1
                PaddingLabel1.Name = "PaddingLabel1"
    function Label1func:Refresh(tochange)
     Label1.Text = tochange
    end
    
    return Label1func
    end
    
            function main:AddSeperatorL(text)
                local Seperator = Instance.new("Frame")
