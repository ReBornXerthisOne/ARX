repeat wait() until game:GetService("Players").LocalPlayer
repeat wait() until game:GetService("ReplicatedStorage").Player_Data
function loadcheck()
    local fileName = "RebornXer Hub Anime Rangers X [" .. game.Players.LocalPlayer.Name .. "].json"
    

    if not isfile(fileName) then
        writefile(fileName, game:GetService("HttpService"):JSONEncode(_G.SST))
        return
    end
end

pcall(function()
    _G.SST = {Select_Vote = "VoteRetry",Auto_Upgrade = false,Auto_Vote = false,Auto_Upgtade_Maxyen = false,Auto_Vote_Start = false,Auto_Play = false,Black_Screen = false,Boost_Fps = false,Select_Farme_Rate = "60",Anti_AFK = true,Auto_Kick_Rejoin = false}
end)


function LoadSetting()
    local fileName = "RebornXer Hub Anime Rangers X [" .. game.Players.LocalPlayer.Name .. "].json"
    if isfile(fileName) then
        local fileContent = readfile(fileName)
        local success, decoded = pcall(function()
            return game:GetService("HttpService"):JSONDecode(fileContent)
        end)
        
        if success then
            _G.SST = decoded
        end
    else
        SS()
    end
end

function SS()
    local fileName = "RebornXer Hub Anime Rangers X [" .. game.Players.LocalPlayer.Name .. "].json"
    
    if isfile(fileName) then
        writefile(fileName, game:GetService("HttpService"):JSONEncode(_G.SST))
    else
        loadcheck()
    end
end

